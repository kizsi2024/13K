<body id="la">

    <header id="la-headbar">
        <div id="head-nav">
            <div id="la-navbar">
                <div id="logoContainer">
                    <div id="la-logo" title="reele"></div>
                </div>
                <div id="lainfobx">
                    <p id="la-name">{{data.name}}</p>
                    <div id="exit" title="Exit"></div>
                </div>
            </div>
            <span class="line-vertical-la"></span>
        </div>
    </header>
    <div id="la-hPadding"></div>

    <main id="container"> 
        <div id="laBX">
            <div id="laHeader">
                <div id="la-header">
                    <div id="la-flag"></div>
                    <p class="header-title">Suspicious posts</p>
                </div>
                <span class="line-vertical-la"></span>
                <div class="la-clubs"></div>
            </div>
        </div>
    </main>

    <script src="./postsGenerator.js"></script>
    <script src="./clubGenerator.js"></script>
    <script src="./aHandler.js"></script>
    <script src="../mini-router/laLog.js"></script>
    <script src="../mini-router/routescript.js"></script>
    <script src="../mini-router/redirect.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
